{
  "name": "@snapapi/sdk",
  "version": "1.2.0",
  "description": "Official JavaScript/TypeScript SDK for SnapAPI - Lightning-fast screenshot API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "screenshot",
    "api",
    "webpage",
    "capture",
    "puppeteer",
    "playwright",
    "url-to-image",
    "web-screenshot",
    "extract",
    "analyze",
    "markdown",
    "snapapi"
  ],
  "author": "SnapAPI <support@snapapi.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sleywill/snapapi-js.git"
  },
  "homepage": "https://snapapi.pics",
  "bugs": {
    "url": "https://github.com/Sleywill/snapapi-js/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
